{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["prettier"],
  "rules": { "prettier/prettier": "error", "react/prop-types": "off" },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
